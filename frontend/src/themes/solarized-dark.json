{
    "colors": {
        "editor.background": "#073642",
        "editor.foreground": "#839496",
        "editorLineNumber.foreground": "#586e75",
        "editorLineNumber.activeForeground": "#93a1a1",
        "editor.selectionBackground": "#586e75",
        "editor.selectionHighlightBackground": "#586e75",
        "editor.inactiveSelectionBackground": "#073642",
        "editor.wordHighlightBackground": "#586e75",
        "editor.wordHighlightStrongBackground": "#586e75",
        "editor.findMatchBackground": "#b58900",
        "editor.findMatchHighlightBackground": "#586e75",
        "editor.findRangeHighlightBackground": "#586e75",
        "editor.hoverHighlightBackground": "#586e75",
        "editor.lineHighlightBackground": "#586e7520",
        "editorCursor.foreground": "#839496",
        "editorWhitespace.foreground": "#586e75",
        "editorIndentGuide.background": "#586e75",
        "editorIndentGuide.activeBackground": "#839496",
        "editorRuler.foreground": "#586e75"
    },
    "rules": [
        {
            "token": "",
            "foreground": "#839496"
        },
        {
            "token": "comment",
            "foreground": "#586e75",
            "fontStyle": "italic"
        },
        {
            "token": "keyword",
            "foreground": "#859900"
        },
        {
            "token": "keyword.control",
            "foreground": "#859900"
        },
        {
            "token": "keyword.operator",
            "foreground": "#859900"
        },
        {
            "token": "string",
            "foreground": "#2aa198"
        },
        {
            "token": "number",
            "foreground": "#d33682"
        },
        {
            "token": "identifier",
            "foreground": "#268bd2"
        },
        {
            "token": "type",
            "foreground": "#b58900"
        },
        {
            "token": "constant",
            "foreground": "#cb4b16"
        },
        {
            "token": "function",
            "foreground": "#268bd2"
        },
        {
            "token": "variable",
            "foreground": "#839496"
        },
        {
            "token": "regexp",
            "foreground": "#dc322f"
        },
        {
            "token": "attribute.name",
            "foreground": "#93a1a1"
        },
        {
            "token": "attribute.value",
            "foreground": "#2aa198"
        },
        {
            "token": "meta.tag",
            "foreground": "#93a1a1"
        },
        {
            "token": "tag",
            "foreground": "#268bd2"
        },
        {
            "token": "delimiter",
            "foreground": "#93a1a1"
        }
    ]
}